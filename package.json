{
  "name": "logni.js",
  "version": "0.2.4",
  "description": "javascript library for event logging and application states",
  "main": "src/logni.js",
  "module": "build/js/lognies5.js",
  "directories": {
    "test": "test",
    "build": "build"
  },
  "scripts": {
    "test": "jshint --verbose --show-non-errors src/logni.js",
    "build": "mkdir -p build/js && ./node_modules/babel-minify/bin/minify.js src/logni.js -o build/js/logni.min.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikni/logni.js.git"
  },
  "keywords": [
    "log",
    "logging",
    "logger",
    "lognijs",
    "logni",
    "js"
  ],
  "author": "Erik Brozek <erik+logni@brozek.name>",
  "license": "GPL3 <https://github.com/erikni/logni.js/blob/master/LICENSE>",
  "bugs": {
    "url": "https://github.com/erikni/logni.js/issues"
  },
  "homepage": "https://logni.net",
  "ignore": [
    "**/.*",
    "node_modules",
    "test",
    "tests"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "npx": "^10.2.0",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.1.3"
  },
  "snyk": true,
  "dependencies": {
    "snyk": "^1.235.1"
  }
}
